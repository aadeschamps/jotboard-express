{
  "name": "jotboard",
  "version": "1.0.0",
  "description": "An application to experiment in real time collaboration",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive test",
    "start": "node ./bin/startApp.js",
    "build": "rm -rf node_modules && npm install",
    "migration": "db-migrate -m ./db/migrations --config ./db/database.json",
    "migrate": "db-migrate -m ./db/migrations --config ./db/database.json up",
    "start:w": "NODE_ENV=dev nodemon --watch lib ./bin/startApp.js",
    "webpack": "webpack",
    "webpack:w": "webpack -d -w"
  },
  "keywords": [
    "jotboard"
  ],
  "author": "Alex Deschamps",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "bcrypt": "^1.0.1",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "config": "^1.21.0",
    "db-migrate": "^0.9.25",
    "express": "^4.13.4",
    "express-promise": "^0.4.0",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "pg-promise": "^4.6.3",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "request": "^2.69.0",
    "socket.io": "^1.4.5",
    "validator": "^6.2.0",
    "webpack": "^1.12.14",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "nodemon": "^1.11.0"
  }
}
