    "morgan": "^1.7.0",
    "pg-promise": "^4.6.3",
    "request": "^2.69.0",
    "socket.io": "^1.4.5",
    "webpack": "^1.12.14",
    "winston": "^2.2.0"
  },
  "devDependencies": {
